{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*.css",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/key",
        "dest": "/index.js"
      },
      {
        "src": "/api/product(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }